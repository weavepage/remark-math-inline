{
  "name": "remark-math-inline",
  "version": "0.1.0",
  "description": "Remark plugin for inline math syntax (:math[...])",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
