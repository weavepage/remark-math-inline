    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-stringify": "^11.0.0",
    "typescript": "^5.7.0",
    "unified": "^11.0.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "remark",
    "unified",
    "math",
    "latex",
    "katex",
    "inline"
